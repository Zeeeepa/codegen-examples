{
  "name": "claude-code-integration",
  "version": "1.0.0",
  "description": "Claude Code Integration & Validation Engine - Advanced AI-powered code validation and debugging system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src/ --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src/ --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --mode development --watch",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "audit-ci --config audit-ci.json",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "clean": "rimraf dist/ coverage/ .nyc_output/",
    "docs": "jsdoc -c jsdoc.json",
    "validate": "npm run lint && npm run type-check && npm run test",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "release": "semantic-release"
  },
  "keywords": [
    "claude",
    "code-validation",
    "ai",
    "static-analysis",
    "security",
    "performance",
    "testing",
    "automation",
    "ci-cd",
    "devops"
  ],
  "author": "Claude Code Integration Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeeeepa/codegen-examples.git",
    "directory": "claude-code-integration"
  },
  "bugs": {
    "url": "https://github.com/Zeeeepa/codegen-examples/issues"
  },
  "homepage": "https://github.com/Zeeeepa/codegen-examples/tree/main/claude-code-integration#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "mongoose": "^7.6.3",
    "sequelize": "^6.33.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "axios": "^1.6.0",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "csv-parser": "^3.0.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.3.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.3",
    "crypto-js": "^4.2.0",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "agenda": "^5.0.0",
    "prom-client": "^15.0.0",
    "opentelemetry-api": "^1.7.0",
    "opentelemetry-sdk-node": "^0.45.0",
    "opentelemetry-instrumentation-express": "^0.34.0",
    "opentelemetry-instrumentation-http": "^0.45.0",
    "opentelemetry-exporter-jaeger": "^1.17.0",
    "compression-webpack-plugin": "^10.0.0",
    "terser-webpack-plugin": "^5.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/morgan": "^1.9.7",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/ws": "^8.5.8",
    "@types/multer": "^1.4.9",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.6",
    "@types/crypto-js": "^4.2.1",
    "@types/node-cron": "^3.0.10",
    "@types/jest": "^29.5.6",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-loader": "^9.5.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-jest": "^27.6.0",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^22.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "nock": "^13.3.8",
    "cypress": "^13.5.0",
    "@cypress/code-coverage": "^3.12.8",
    "nyc": "^15.1.0",
    "c8": "^8.0.1",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "typedoc": "^0.25.3",
    "audit-ci": "^6.6.1",
    "npm-check-updates": "^16.14.6",
    "license-checker": "^25.0.1",
    "bundlesize": "^0.18.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "source-map-explorer": "^2.5.3",
    "madge": "^6.1.0",
    "dependency-cruiser": "^14.1.0",
    "retire": "^4.0.3",
    "snyk": "^1.1244.0"
  },
  "peerDependencies": {
    "docker": ">=20.10.0",
    "kubernetes": ">=1.25.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/*.(test|spec).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testTimeout": 30000
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "@typescript-eslint/recommended",
      "plugin:security/recommended",
      "plugin:jest/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "security",
      "jest"
    ],
    "env": {
      "node": true,
      "jest": true,
      "es2022": true
    },
    "rules": {
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "no-console": "warn",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "security/detect-object-injection": "off",
      "security/detect-non-literal-fs-filename": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitizen --hook || true",
      "pre-push": "npm run validate"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/bundle.js",
      "maxSize": "500 kB"
    }
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.test.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}

